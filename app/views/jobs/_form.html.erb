<%= form_for [@job] do |f| %>
<p id='company_dropdown'>
  <%= f.label :company %><br/>
  <%= f.collection_select :company, Company.all, :id, :name%>
</p>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :level_of_interest %>
  <%= f.number_field :level_of_interest %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.submit %>
<% end %>
